{
    "version": 2,
    "builds": [
      {
        "src": "src/main.server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": "dist/listrace/**/*"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }  